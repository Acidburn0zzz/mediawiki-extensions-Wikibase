{
  "name": "data-bridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve src/mock-entry.ts",
    "prepare": "node build/wikimedia-ui-base.js",
    "build": "npm-run-all build:*",
    "build:app": "NODE_ENV=production vue-cli-service build --no-clean --target lib --formats commonjs src/main.ts",
    "build:init": "NODE_ENV=production vue-cli-service build --no-clean",
    "fix": "vue-cli-service lint --max-warnings 0 .",
    "test": "npm-run-all test:*",
    "test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
    "test:unit": "vue-cli-service test:unit",
    "test:integration": "vue-cli-service test:unit --testMatch '**/tests/integration/**/*.spec.(js|jsx|ts|tsx)'",
    "test:distnodiff": "npm run build:app -- --dest /tmp/dist && npm run build:init -- --dest /tmp/dist && diff -q ./dist/data-bridge.common.js /tmp/dist/data-bridge.common.js && diff -q ./dist/data-bridge.init.js /tmp/dist/data-bridge.init.js",
    "selenium-test": "wdio tests/selenium/wdio.conf.js",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.8",
    "@storybook/addon-links": "^5.1.8",
    "@storybook/addon-storysource": "^5.1.9",
    "@storybook/addons": "^5.1.8",
    "@storybook/vue": "^5.1.8",
    "@types/jest": "^23.1.4",
    "@types/jquery": "^3.3.30",
    "@types/node": "^12.7.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@wdio/cli": "^5.11.13",
    "@wdio/local-runner": "^5.11.13",
    "@wdio/mocha-framework": "^5.11.0",
    "@wdio/spec-reporter": "^5.11.7",
    "@wdio/sync": "^5.11.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.3.1",
    "eslint": "^5.16.0",
    "eslint-config-wikimedia": "^0.13.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-plugin-wdio": "^5.11.0",
    "fork-ts-checker-webpack-plugin": "^1.4.3",
    "gonzales-pe": "^4.2.4",
    "http-status-codes": "^1.3.2",
    "less-variable-resolver": "^0.1.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-prefixwrap": "^1.6.0",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "sast": "^0.8.1",
    "stylelint": "^10.1.0",
    "stylelint-config-wikimedia": "^0.6.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.9.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.5.3",
    "url-search-params-polyfill": "^5.1.0",
    "vue-cli-plugin-typescript": "0.0.1",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "wdio-mediawiki": "0.4.0",
