{
	"name": "WikibaseLib",
	"type": "wikibase",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikibaseLib",
	"descriptionmsg": "wikibase-lib-desc",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.35"
	},
	"Hooks": {
		"UnitTestsList": "\\Wikibase\\LibHooks::registerPhpUnitTests",
		"ResourceLoaderTestModules": "\\Wikibase\\LibHooks::registerQUnitTests",
		"ResourceLoaderRegisterModules": "\\Wikibase\\LibHooks::onResourceLoaderRegisterModules",
		"ExtensionTypes": "\\Wikibase\\LibHooks::onExtensionTypes"
	},
	"callback": "\\Wikibase\\LibHooks::onRegistration",
	"MessagesDirs": {
		"WikibaseLib": [
			"lib/i18n"
		]
	},
	"AutoloadNamespaces": {
		"Wikibase\\Lib\\": "lib/includes/",
		"Wikibase\\DataAccess\\": "data-access/src/"
	},
	"TestAutoloadNamespaces": {
		"Wikibase\\Lib\\Tests\\": "lib/tests/phpunit/",
		"Wikibase\\DataAccess\\Tests\\": "data-access/tests/phpunit/"
	},
	"manifest_version": 2
}
